<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <Version>1.0.0</Version>
	  <Authors>Jaine.ch</Authors>
	  <Owners>Jaine.ch</Owners>
	  <Title>HttpClient extension for download file</Title>
	  <Description>
		  HttpClient extension for download file
		  Usage:
			1. using(var client = new HttpClient())
			   {
					await client.DownloadFileAsync(url, filename);
			   }
			2. string url = "some url";
			   await url.DonwloadFileAsync(filename);
	  </Description>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <releaseNotes>v1.0.0</releaseNotes>
	  <RepositoryUrl>http://www.github.com/doeasier/HttpDownload.git</RepositoryUrl>
  </PropertyGroup>

</Project>
